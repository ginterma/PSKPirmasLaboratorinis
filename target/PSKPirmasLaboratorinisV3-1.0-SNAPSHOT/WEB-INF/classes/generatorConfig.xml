<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="CarShopDB" targetRuntime="MyBatis3Simple">
        <jdbcConnection driverClass="org.h2.Driver"
                        connectionURL="jdbc:h2:~/h2database/MaximaDB;AUTO_SERVER=TRUE">
        </jdbcConnection>
        <javaModelGenerator targetPackage="lt.vu.mybatis.model" targetProject="MAVEN" />
        <sqlMapGenerator targetPackage="lt.vu.mybatis.xml" targetProject="MAVEN" />
        <javaClientGenerator type="XMLMAPPER" targetPackage="lt.vu.mybatis.dao" targetProject="MAVEN" />
        <table schema="PUBLIC" tableName="Employee">
            <generatedKey column="Employee_id" identity="true" sqlStatement="JDBC" />
        </table>
        <table schema="PUBLIC" tableName="GROCERIES">
            <generatedKey column="Grocery_id" identity="true" sqlStatement="JDBC" />
        </table>
    </context>
</generatorConfiguration>